{
  "name": "node-ts-starter",
  "version": "1.0.0",
  "description": "A starter for the Node.js project with TypeScript and pnpm",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodm/node-ts-starter.git"
  },
  "scripts": {
    "start": "node build/index.js",
    "dev": "pnpm run \"/dev:/\"",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "dev:node": "node --enable-source-maps --watch build/index.js",
    "build": "pnpm compile",
    "test": "pnpm jest",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "pnpm compile",
    "pretest": "pnpm compile",
    "posttest": "pnpm lint"
  },
  "keywords": [
    "Node.js",
    "TypeScript",
    "pnpm"
  ],
  "author": "nodm",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "gts": "^5.3.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3"
  }
}
